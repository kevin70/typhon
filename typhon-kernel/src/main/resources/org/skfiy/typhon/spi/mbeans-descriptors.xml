<!DOCTYPE mbeans-descriptors PUBLIC
	 "-//Apache Software Foundation//DTD Model MBeans Configuration File"
	 "http://skfiy.googlecode.com/files/mbeans-descriptors.dtd">
         
<mbeans-descriptors>
    
    <mbean name="ConfigurationLoader"
           description="基础配置数据加载器"
           domain="org.skfiy.typhon"
           type="org.skfiy.typhon.spi.ConfigurationLoader">
        
        <operation name="reload"
                   description="reload this ConfigurationLoader"
                   impact="ACTION"
                   returnType="void"/>
    </mbean>
    
    <mbean name="ItemProvider"
           description="道具提供对象"
           domain="org.skfiy.typhon"
           type="org.skfiy.typhon.spi.ItemProvider">
        
        <operation name="reload"
                   description="reload this ItemProvider"
                   impact="ACTION"
                   returnType="void"/>
    </mbean>
    
</mbeans-descriptors>